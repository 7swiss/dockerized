[sshd]
backend = polling
journalmatch =
enabled     = true
filter      = sshd
logpath     = /var/log/messages tail
action      = iptables-multiport[name=sshd, port="22", protocol=tcp]
              banned_db[name=sshd, port="22", protocol=tcp]
